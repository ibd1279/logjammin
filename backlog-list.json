<?lc if backlogs and table.getn(backlogs) > 0 then ?> [
<?lc for h, backlog in ipairs(backlogs) do ?>${h == 1 and "" or ","}{
    pkey:${backlog:pkey()},
    brief:"${_json(backlog:brief())}",
    version:"${_json(backlog:version())}",
    category:"${_json(backlog:category())}",
    disposition:"${_json(backlog:disposition())}",
    estimate:${backlog:estimate()}
}<?lc end ?>
] <?lc else ?> [] <?lc end ?>
